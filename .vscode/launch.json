{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch (Demo)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/samples/HotReloadDemo.Desktop/bin/Debug/net10.0/HotReloadDemo.Desktop.dll",
      "args": [],
      "cwd": "${workspaceFolder}/samples/HotReloadDemo.Desktop",
      "console": "internalConsole",
      "stopAtEntry": false,
      "logging": {
        "moduleLoad": false,
      }
    },
    {
      "name": "Mono Launch (Android Demo)",
      "type": "mono",
      "preLaunchTask": "android-run-demo",
      "request": "attach",
      "address": "localhost",
      "port": 19680,
    },
    {
      "name": ".NET Attach",
      "type": "coreclr",
      "request": "attach",
    },
    {
      "name": "Mono Attach",
      "type": "mono",
      "request": "attach",
      "address": "localhost",
      "port": 19680,
    }
  ],
}
